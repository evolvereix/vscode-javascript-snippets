{
  "name": "javascript-snippets",
  "displayName": "JavaScript Snippets",
  "description": "JavaScript(ES6+) syntax code snippets for VS Code",
  "version": "0.3.3",
  "publisher": "algorizen",
  "author": "Evolvereix",
  "license": "MIT",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.29.0"
  },
  "keywords": [
    "js",
    "ts",
    "javascript",
    "typescript",
    "vue",
    "snippet"
  ],
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "homepage": "https://github.com/evolvereix/vscode-javascript-snippets#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evolvereix/vscode-javascript-snippets.git"
  },
  "bugs": {
    "url": "https://github.com/evolvereix/vscode-javascript-snippets/issues"
  },
  "activationEvents": [
    "onCommand:extension.insertConsoleLogStatement"
  ],
  "main": "./extension",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "vue",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "html",
        "path": "./snippets/javascript.json"
      }
    ],
    "commands": [
      {
        "command": "extension.insertConsoleLogStatementDown",
        "title": "JavaScript Snippets: Insert The console.log Statement"
      }
    ],
    "keybindings": [
      {
        "command": "extension.insertConsoleLogStatementDown",
        "mac": "ctrl+cmd+l",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.insertConsoleLogStatementUp",
        "mac": "shift+ctrl+cmd+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "vitepress": "1.0.0-alpha.65",
    "vscode": "^1.1.37"
  }
}
