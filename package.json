{
  "name": "javascript-snippets",
  "displayName": "JavaScript Snippets",
  "description": "JavaScript(ES6+) syntax code snippets for VS Code",
  "version": "0.1.8",
  "publisher": "algorizen",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/algorizen/vscode-javascript-snippets.git"
  },
  "scripts": {
    "preinstall": "node ./scripts/checkInstallTool.js",
    "release": "node ./scripts/release.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "keywords": [
    "js",
    "JavaScript",
    "code",
    "snippets"
  ],
  "author": "algorizen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/algorizen/vscode-javascript-snippets/issues"
  },
  "homepage": "https://github.com/algorizen/vscode-javascript-snippets#readme",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "html",
        "path": "./snippets/snippets.json"
      },
      {
        "language": "vue",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "devDependencies": {
    "chalk": "^4.1.2",
    "conventional-changelog-cli": "^2.1.1",
    "enquirer": "^2.3.6",
    "execa": "^5.1.1",
    "minimist": "^1.2.5",
    "semver": "^7.3.5",
    "standard-version": "^9.3.1"
  }
}
